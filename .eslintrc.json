{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "ignorePatterns": ["__tests__/*", "src/database/*"],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error"]
    }
}
